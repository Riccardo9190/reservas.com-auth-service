generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  email     String   @unique
  password  String
  role      String
  createdAt DateTime @default(now()) @db.Timestamp(0)

  @@map("user")
}
